pages = {184--93},
pmid = {17894353},
title = {{Prediction of global and local model quality in CASP7 using Pcons and ProQ.}},
url = {http://doi.wiley.com/10.1002/prot.21774 http://www.ncbi.nlm.nih.gov/pubmed/17894353},
