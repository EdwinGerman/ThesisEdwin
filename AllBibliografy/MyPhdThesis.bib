@article{Bryant1995,
abstract = {The past two years have seen the rapid development of new recognition methods for protein structure prediction. These algorithms 'thread' the sequence of one protein through the known structure of another, looking for an alignment that corresponds to an energetically favorable model structure. Because they are based on energy calculation, rather than evolutionary distance, these methods extend the possibility of structure prediction by comparative modeling to a larger class of new sequences, where similarity to known structures is recognizable by no other means. The strength of the evidence they offer should be judged by objective statistical tests, however, so as to rule out the possibility that favorable scores arise from chance factors such as similarity of length, composition, or the consideration of a large number of alternative alignments. Calculation of objective p-values by analytical means is not yet possible, but it would appear that approximate values may be obtained by simulation, as they are in gapped, global sequence alignment. We propose that the results of threading experiments should include Z-scores relative to the composition-corrected score distribution obtained for shuffled and optimally aligned sequences. {\textcopyright} 1995.},
author = {Bryant, Stephen H. and Altschul, Stephen F.},
doi = {10.1016/0959-440X(95)80082-4},
file = {:home/edwin/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bryant, Altschul - 1995 - Statistics of sequence-structure threading.pdf:pdf},
isbn = {0959-440X (Print)$\backslash$r0959-440X (Linking)},
issn = {0959440X},
journal = {Current Opinion in Structural Biology},
number = {2},
pages = {236--244},
pmid = {7648327},
title = {{Statistics of sequence-structure threading}},
url = {http://www.sciencedirect.com/science/article/pii/0959440X95800824},
volume = {5},
year = {1995}
}
@article{Benkert2009,
abstract = {Identifying the best candidate model among an ensemble of alternatives is crucial in protein structure prediction. For this purpose, scoring functions have been developed which either calculate a quality estimate on the basis of a single model or derive a score from the information contained in the ensemble of models generated for a given sequence (i.e., consensus methods). At CASP7, consensus methods have performed considerably better than scoring functions operating on single models. However, consensus methods tend to fail if the best models are far from the center of the dominant structural cluster. At CASP8, we investigated whether our hybrid method QMEANclust may overcome this limitation by combining the QMEAN composite scoring function operating on single models with consensus information. We participated with four different scoring functions in the quality assessment category. The QMEANclust consensus scoring function turned out to be a successful method both for the ranking of entire models but especially for the estimation of the per-residue model quality. In this article, we briefly describe the two scoring functions QMEAN and QMEANclust and discuss their performance in the context of what went right and wrong at CASP8. Both scoring functions are publicly available at http://swissmodel.expasy.org/qmean/.},
author = {Benkert, Pascal and Tosatto, Silvio C E and Schwede, Torsten},
doi = {10.1002/prot.22532},
file = {:home/edwin/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Benkert, Tosatto, Schwede - 2009 - Global and local model quality estimation at CASP8 using the scoring functions QMEAN and QMEANclust.pdf:pdf},
isbn = {10970134},
issn = {1097-0134},
journal = {Proteins},
keywords = {CASP8,Mean force potential,Model quality assessment,Protein structure homology modeling,QMEAN,Scoring function},
number = {SUPPL. 9},
pages = {173--80},
pmid = {19705484},
title = {{Global and local model quality estimation at CASP8 using the scoring functions QMEAN and QMEANclust.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19705484},
volume = {77 Suppl 9},
year = {2009}
}
