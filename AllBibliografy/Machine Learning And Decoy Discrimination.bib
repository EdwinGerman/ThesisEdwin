issn = {0961-8368},
journal = {Protein science : a publication of the Protein Society},
keywords = {comparative modeling,fold assignment,genomics efforts are providing,model assessment,org,protein structure prediction,proteinscience,researchers with the,see www,statistical potentials,supplemental material,support,vector machine},
number = {7},
pages = {1653--1666},
