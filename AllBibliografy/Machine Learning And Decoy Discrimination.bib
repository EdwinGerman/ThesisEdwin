journal = {Bioinformatics (Oxford, England)},
number = {15},
pages = {2221--2223},
pmid = {24713439},
title = {{ProQM-resample: Improved Model Quality Assessment for Membrane Proteins by Limited Conformational Sampling.}},
url = {http://bioinformatics.oxfordjournals.org/content/early/2014/04/07/bioinformatics.btu187.abstract.html?papetoc},
