issn = {0219-7200},
journal = {Journal of Bioinformatics and Computational Biology},
keywords = {feature importance,ing,machine learn-,physicochemical properties of protein,protein structure prediction,random forest,sade},
number = {02},
pages = {1550005},
title = {{Quality assessment of modeled protein structure using physicochemical properties}},
url = {http://www.worldscientific.com/doi/abs/10.1142/S0219720015500055},
volume = {13},
year = {2015}
}
