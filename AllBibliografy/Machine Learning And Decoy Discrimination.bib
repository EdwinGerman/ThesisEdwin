file = {:home/edwin/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Zhou et al. - 2014 - Amino Acid Network for the Discrimination of Native Protein Structures from Decoys.pdf:pdf},
issn = {1875-5550},
