doi = {10.1186/1471-2105-15-120},
file = {:home/edwin/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Cao et al. - 2014 - SMOQ a tool for predicting the absolute residue-specific quality of a single protein model with support vector machi.pdf:pdf},
issn = {1471-2105},
journal = {BMC bioinformatics},
number = {1},
pages = {120},
pmid = {24776231},
title = {{SMOQ: a tool for predicting the absolute residue-specific quality of a single protein model with support vector machines.}},
