file = {:home/edwin/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Mart{\'{i}}-Renom et al. - 2000 - Comparative protein structure modeling of genes and genomes.pdf:pdf},
isbn = {1056-8700},
issn = {1056-8700},
journal = {Annual review of biophysics and biomolecular structure},
keywords = {alignment,alignment to one or,based primarily on its,fold assignment,fully automated modeling,given protein sequence,homology,model evaluation,modeling,more pro-,of a,predicts the three-dimensional structure,protein structure prediction,s abstract comparative modeling,structural genomics,target},
pages = {291--325},
